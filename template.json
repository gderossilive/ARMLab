{
    "$schema": "https://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "virtualMachines_PCLVM1721101457_name": {
            "defaultValue": "PCLVM1721101457",
            "type": "String"
        },
        "diskEncryptionSets_PCLDiskEncryptionSet1721101457_externalid": {
            "defaultValue": "/subscriptions/594cafab-484b-40b0-8336-f0a6042a8754/resourceGroups/PCLRG1721101457/providers/Microsoft.Compute/diskEncryptionSets/PCLDiskEncryptionSet1721101457",
            "type": "String"
        },
        "disks_PCLVM1721101457_OsDisk_1_8277c6a5b806405fbddb92a9deb6d471_externalid": {
            "defaultValue": "/subscriptions/594cafab-484b-40b0-8336-f0a6042a8754/resourceGroups/PCLRG1721101457/providers/Microsoft.Compute/disks/PCLVM1721101457_OsDisk_1_8277c6a5b806405fbddb92a9deb6d471",
            "type": "String"
        },
        "disks_MyVM_data0_externalid": {
            "defaultValue": "/subscriptions/594cafab-484b-40b0-8336-f0a6042a8754/resourceGroups/PCLRG1721101457/providers/Microsoft.Compute/disks/MyVM-data0",
            "type": "String"
        },
        "disks_MyVM_data1_externalid": {
            "defaultValue": "/subscriptions/594cafab-484b-40b0-8336-f0a6042a8754/resourceGroups/PCLRG1721101457/providers/Microsoft.Compute/disks/MyVM-data1",
            "type": "String"
        },
        "networkInterfaces_myNic_externalid": {
            "defaultValue": "/subscriptions/594cafab-484b-40b0-8336-f0a6042a8754/resourceGroups/PCLRG1721101457/providers/Microsoft.Network/networkInterfaces/myNic",
            "type": "String"
        }
    },
    "variables": {},
    "resources": [
        {
            "type": "Microsoft.Compute/virtualMachines",
            "apiVersion": "2019-07-01",
            "name": "[parameters('virtualMachines_PCLVM1721101457_name')]",
            "location": "westeurope",
            "properties": {
                "hardwareProfile": {
                    "vmSize": "Standard_B8ms"
                },
                "storageProfile": {
                    "imageReference": {
                        "publisher": "Canonical",
                        "offer": "UbuntuServer",
                        "sku": "14.04.2-LTS",
                        "version": "latest"
                    },
                    "osDisk": {
                        "osType": "Linux",
                        "name": "[concat(parameters('virtualMachines_PCLVM1721101457_name'), '_OsDisk_1_8277c6a5b806405fbddb92a9deb6d471')]",
                        "createOption": "FromImage",
                        "caching": "ReadWrite",
                        "managedDisk": {
                            "diskEncryptionSet": {
                                "id": "[parameters('diskEncryptionSets_PCLDiskEncryptionSet1721101457_externalid')]"
                            },
                            "storageAccountType": "Premium_LRS",
                            "id": "[parameters('disks_PCLVM1721101457_OsDisk_1_8277c6a5b806405fbddb92a9deb6d471_externalid')]"
                        },
                        "diskSizeGB": 30
                    },
                    "dataDisks": [
                        {
                            "lun": 0,
                            "name": "MyVM-data0",
                            "createOption": "Empty",
                            "caching": "None",
                            "managedDisk": {
                                "diskEncryptionSet": {
                                    "id": "[parameters('diskEncryptionSets_PCLDiskEncryptionSet1721101457_externalid')]"
                                },
                                "storageAccountType": "Premium_LRS",
                                "id": "[parameters('disks_MyVM_data0_externalid')]"
                            },
                            "diskSizeGB": 1000,
                            "toBeDetached": false
                        },
                        {
                            "lun": 1,
                            "name": "MyVM-data1",
                            "createOption": "Empty",
                            "caching": "None",
                            "managedDisk": {
                                "diskEncryptionSet": {
                                    "id": "[parameters('diskEncryptionSets_PCLDiskEncryptionSet1721101457_externalid')]"
                                },
                                "storageAccountType": "Premium_LRS",
                                "id": "[parameters('disks_MyVM_data1_externalid')]"
                            },
                            "diskSizeGB": 1000,
                            "toBeDetached": false
                        }
                    ]
                },
                "osProfile": {
                    "computerName": "[parameters('virtualMachines_PCLVM1721101457_name')]",
                    "adminUsername": "gdradmin",
                    "linuxConfiguration": {
                        "disablePasswordAuthentication": false,
                        "provisionVMAgent": true
                    },
                    "secrets": [],
                    "allowExtensionOperations": true,
                    "requireGuestProvisionSignal": true
                },
                "networkProfile": {
                    "networkInterfaces": [
                        {
                            "id": "[parameters('networkInterfaces_myNic_externalid')]"
                        }
                    ]
                },
                "diagnosticsProfile": {
                    "bootDiagnostics": {
                        "enabled": true,
                        "storageUri": "https://armlabstore.blob.core.windows.net/"
                    }
                }
            }
        }
    ]
}